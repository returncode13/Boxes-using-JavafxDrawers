<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>

<fx:root id="AnchorPane" maxHeight="150.0" maxWidth="500.0" minHeight="150.0" minWidth="200.0" prefHeight="150.0" prefWidth="500.0" type="AnchorPane" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fend.project.ProjectController">
   <children>
      <HBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" HBox.hgrow="NEVER" />
            <GridPane>
               <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="300.0" minWidth="10.0" prefWidth="300.0" />
                  <ColumnConstraints hgrow="NEVER" maxWidth="20.0" minWidth="20.0" prefWidth="20.0" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
               </rowConstraints>
               <children>
                  <HBox alignment="CENTER" GridPane.rowIndex="1">
                     <children>
                        <Label alignment="CENTER" text="Choose the project" />
                     </children>
                  </HBox>
                  <HBox GridPane.rowIndex="2">
                     <children>
                        <ComboBox fx:id="databaseComboBox" prefWidth="300.0" HBox.hgrow="ALWAYS" />
                     </children>
                  </HBox>
                  <HBox GridPane.rowIndex="4">
                     <children>
                        <Pane layoutX="47.0" layoutY="10.0" prefWidth="175.0" HBox.hgrow="NEVER" />
                      <Button fx:id="ok" mnemonicParsing="false" onAction="#ok" text="OK" />
                        <Pane HBox.hgrow="ALWAYS" />
                        <Button fx:id="exit" mnemonicParsing="false" onAction="#exit" text="Exit" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" />
                  <Button fx:id="dbsettings" maxWidth="20.0" minWidth="20.0" mnemonicParsing="false" onAction="#dbsettings" prefWidth="20.0" text="Button" GridPane.columnIndex="1" />
               </children>
            </GridPane>
         </children>
      </HBox>
   </children>
</fx:root>
