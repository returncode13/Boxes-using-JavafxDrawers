<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>

<fx:root id="AnchorPane" maxHeight="150.0" maxWidth="500.0" minHeight="150.0" minWidth="200.0" prefHeight="150.0" prefWidth="500.0" type="AnchorPane" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fend.project.ProjectController">
   <children>
      <GridPane style="-fx-background-color: #3e444c;" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
            <ColumnConstraints hgrow="NEVER" maxWidth="20.0" minWidth="20.0" prefWidth="20.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <HBox alignment="CENTER" GridPane.rowIndex="1">
               <children>
                  <Label alignment="CENTER" style="-fx-text-fill: white;" text="Choose the project" textFill="#afb82700" />
               </children>
            </HBox>
            <HBox alignment="CENTER" GridPane.rowIndex="2">
               <children>
                  <ComboBox fx:id="databaseComboBox" prefWidth="300.0" style="-fx-background-color: #555d68;" stylesheets="@../../css/combo.css" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
            <HBox GridPane.rowIndex="4">
               <children>
                  <Pane layoutX="47.0" layoutY="10.0" HBox.hgrow="ALWAYS" />
                <Button fx:id="ok" mnemonicParsing="false" onAction="#ok" style="-fx-min-width: 50; -fx-max-width: 50;" styleClass="primary" stylesheets="@../../styles/bootstrap3.css" text="OK" />
                  <Pane prefWidth="30.0" HBox.hgrow="NEVER" />
                  <Button fx:id="exit" mnemonicParsing="false" onAction="#exit" style="-fx-min-width: 50; -fx-max-width: 50;" styleClass="danger" stylesheets="@../../styles/bootstrap3.css" text="Exit" />
               </children>
            </HBox>
            <HBox alignment="CENTER" />
         </children>
      </GridPane>
   </children>
</fx:root>
